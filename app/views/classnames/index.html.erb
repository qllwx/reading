<p id="notice"><%= notice %></p>

<h1>班级</h1>

<table>
  <thead>
    <tr>
      <th><%= t('Classname.classname') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classnames.each do |classname| %>
      <tr>
        <td><%= classname.classname %></td>
        <td><%= link_to t('Show'), classname %></td>
        <td><%= link_to t('Edit'), edit_classname_path(classname) %></td>
        <td><%= link_to t('Destroy'), classname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @classnames %>
<%= link_to t('New'), new_classname_path %>
